## 389. XOR Operation in an Array

public class codefile{
    static int solve(int n, int start){
		int nums[]=new int[n];
        for(int i=0;i<n;i++){
            nums[i]=start+(2*i);
        }
        int xr=0;
        for(int i=0;i<nums.length;i++){
            xr=xr^nums[i];
        }
        return xr;
    }
}
